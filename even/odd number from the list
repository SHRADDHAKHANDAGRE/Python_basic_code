def findGrade(m):
    if(m<=100 and m>90):
        print("S Grade")
    elif(m<=90 and m>80):
        print("A Grade")
    elif(m<=80 and m>70):
        print("B Grade")
    elif(m<=70 and m>60):
        print("C Grade")
    elif(m<=60 and m>50):
        print("D Grade")
    elif(m<=50 and m>40):
        print("E Grade")
    elif(m<=40 and m>0):
        print("Student has failed")
    else:
        print("Invalid marks")
        
m = int(input("Enter your marks:"))

findGrade(m)
