list = [1, 2, 3 , 43 , 543 ,  46, 46 , 1, 23, 3, 3 ,2, 3, 21,3,4,1,6,6,9]

my = list.count(3)
print(my)

n = 3 
s = 0
for i in range (0,len(list)):
    if(list[i]==n):
        s = s+1 
print(s)

for ele in list:
    if(ele==n):
        s = s+1 
print("{} has occured {} times".format(n,s))

from collections import Counter
dic = Counter(list)
print("{} has occured {} times".format(n,dic[n]))
